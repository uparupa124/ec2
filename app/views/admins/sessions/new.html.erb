<div class="container">
 <div class="d-flex align-items-center" style="width:100%; height:100px;">
  <h3><span class="bg-light">管理者ログイン</span></h3>
 </div>
   <table>
     <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <tr>
       <div class="field">
         <td>
          <%= f.label :"メールアドレス" %>
         </td>
          <td>
            <%= f.email_field :email, autofocus: true, autocomplete: "email", style:"width:300px;" %>
          </td>
       </div>
      </tr>
        <tr>
          <div class="field">
            <td>
              <%= f.label :"パスワード" %>
            </td>
              <td>
               <%= f.password_field :password, autocomplete: "current-password", style:"width:300px;" %>
              </td>
        </tr>
  </div>
   </table>

  <!--<% if devise_mapping.rememberable? %>-->
    <!--<div class="field">-->
      <!--<%= f.check_box :remember_me %>-->
      <!--<%= f.label :remember_me %>-->
    <!--</div>-->
  <!--<% end %>-->
<br>
  <div class="actions">
    <%= f.submit "ログイン", class:"btn btn-primary" %>
  </div>
<% end %>
   

<!--<%= render "devise/shared/links" %>-->
</div>